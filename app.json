{
  "expo": {
    "name": "FoodBuddy",
    "slug": "FoodBuddy",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/AndroidIcon.png",
        "backgroundColor": "#F8D64E"
      },
      "package": "com.code_celestia.FoodBuddy"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/FoodBuddySmall.png",
          "imageWidth": 150,
          "resizeMode": "contain",
          "backgroundColor": "#F8D64E"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "5455a261-0501-4972-81e2-4de97a7651cf"
      }
    }
  }
}
